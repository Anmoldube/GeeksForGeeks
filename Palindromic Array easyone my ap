
class GfG {
    public static int palinArray(int[] a, int n) {
        for (int i = 0; i < n; i++) {
            int num = a[i];
            int sum = 0;
            int originalNum = num;
            while (num > 0) {
                int rev = num % 10;
                sum = sum * 10 + digit;
                num /= 10;
            }
            if (originalNum != sum) {
                return 0; 
            }
        }
        return 1; 
    }
}
